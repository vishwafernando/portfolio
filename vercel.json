{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.vishwafernando.me"
        }
      ],
      "destination": "https://vishwafernando.me/:1",
      "permanent": true
    }
  ]
}
// This configuration file redirects all traffic from www.vishwafernando.me to vishwafernando.me//